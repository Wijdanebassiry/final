@model WebApplication6.Models.FicheProjet
@{
    ViewData["Title"] = "Modifier la Fiche Projet";
}
<h2>@ViewData["Title"]</h2>
<form asp-action="Edit" method="post">
    <input type="hidden" asp-for="Id" />
    <input type="hidden" asp-for="Detail.Id" />
    <div class="form-group">
        <label asp-for="NumeroProjet"></label>
        <input asp-for="NumeroProjet" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="NumeroAffaire"></label>
        <input asp-for="NumeroAffaire" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ContratNumero"></label>
        <input asp-for="ContratNumero" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="NomClient"></label>
        <input asp-for="NomClient" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="TelephoneContact"></label>
        <input asp-for="TelephoneContact" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="AdresseLivraison"></label>
        <input asp-for="AdresseLivraison" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ModalitePaiement"></label>
        <input asp-for="ModalitePaiement" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DernierDelaiLivraison"></label>
        <input asp-for="DernierDelaiLivraison" type="date" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DernierDelaiExecution"></label>
        <input asp-for="DernierDelaiExecution" type="date" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DureeGarantie"></label>
        <input asp-for="DureeGarantie" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DetailFormation"></label>
        <input asp-for="DetailFormation" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DossierAMSSNUR"></label>
        <input asp-for="DossierAMSSNUR" type="checkbox" class="form-check-input" />
    </div>
    <div class="form-group">
        <label asp-for="ContratMaintenance"></label>
        <input asp-for="ContratMaintenance" type="checkbox" class="form-check-input" />
    </div>
    <h4>DÃ©tail du projet</h4>
    <div class="form-group">
        <label asp-for="Detail.Equipements"></label>
        <input asp-for="Detail.Equipements" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Detail.Accessoires"></label>
        <input asp-for="Detail.Accessoires" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Detail.TravauxPromamec"></label>
        <input asp-for="Detail.TravauxPromamec" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Detail.Commentaires"></label>
        <input asp-for="Detail.Commentaires" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Enregistrer</button>
    <a asp-action="Index" class="btn btn-secondary">Annuler</a>
</form> 